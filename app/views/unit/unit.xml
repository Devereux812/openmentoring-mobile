<Page xmlns="http://www.nativescript.org/tns.xsd" navigatedTo="pageNavigatedTo">
  <Page.actionBar>
    <ActionBar title="Unit">
      <ActionBar.actionItems>
        <ActionItem text="Back" tap="buttonBackTap"  ios.position="left"/>
      </ActionBar.actionItems>
    </ActionBar>
  </Page.actionBar>
  <DockLayout stretchLastChild="true">
    <Button dock="left" text="back" />
    <Button dock="right" text="forward" />
    <HtmlView html="{{ htmlString }}" class="card" />
  </DockLayout>
</Page>
